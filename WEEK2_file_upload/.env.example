# Azure Foundry Agent Configuration
# Copy this file to .env and fill in your values

# Your Azure Foundry project endpoint
FOUNDRY_PROJECT_ENDPOINT=https://proj-ais-eastus2-azure-foundry-t.services.ai.azure.com/api/projects/proj-ais-eastus2-azure-foundry-test
AZURE_AI_PROJECT_ENDPOINT=""

# The name of your agent in Azure Foundry
FOUNDRY_AGENT_NAME=2026-1-12-KnowledgeAgentTest02

# Authentication method
# Set to 'true' if using API key, 'false' for DefaultAzureCredential
FOUNDRY_USE_KEY_AUTH=false

# API Key (only required if FOUNDRY_USE_KEY_AUTH is true)
# Get this from your Azure Foundry project settings
# FOUNDRY_API_KEY=your-api-key-here

# Azure authentication (used with DefaultAzureCredential)
# These are optional - DefaultAzureCredential will use your system's Azure CLI login
# Uncomment and set if needed:
# AZURE_SUBSCRIPTION_ID=your-subscription-id
# AZURE_TENANT_ID=your-tenant-id
# AZURE_CLIENT_ID=your-client-id
# AZURE_CLIENT_SECRET=your-client-secret
